<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID" version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  <filter>
    <filter-name>FilterDistribute</filter-name>
    <filter-class>com.hui.controller.filter.FilterDistribute</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>FilterDistribute</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>

  <filter>
    <filter-name>FilterBooks</filter-name>
    <filter-class>com.hui.controller.filter.FilterBooks</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>FilterBooks</filter-name>
    <url-pattern>/books/*</url-pattern>
  </filter-mapping>

  <welcome-file-list>
    <welcome-file>/jsp/login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
